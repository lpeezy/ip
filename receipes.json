        "beef": [
            {
                "title": "Kitchen-Sink Pot Roast",
                "credit": "Jessie Van Slyke",
                "creditURL": "https://www.kitschencat.com/pressure-cooker-kitchen-sink-pot-roast/",
                "serveSize": "6-8",
                "cookTime": "2 hours",
                "img": "img/PC-Kitchen-Sink-Pot-Roast-5.jpg",
                "ingredients": [
                    {
                        "ing1": "1 3 1/2 to 4 lb boneless beef rump roast",
                        "ing2": "1 teaspoon salt",
                        "ing3": "1 teaspoon dried thyme",
                        "ing4": "1/2 teaspoon black pepper",
                        "ing5": "2 tablespoons vegetable oil",
                        "ing6": "1 1/2 cups beef broth",
                        "ing7": "1 cup dry red wine",
                        "ing8": "4 medium Yukon gold potatoes, halved (unpeeled)",
                        "ing9": "2 medium yellow onions, quartered",
                        "ing10": "4 large carrots, cut into 2-inch pieces",
                        "ing11": "4 stalks celery, cut into 2-inch lengths",
                        "ing12": "4 cups baby kale",
                        "ing13": "",
                        "ing14": "",
                        "ing15": ""
                    }
                ],
                "instructions": [
                    {
                        "step1": "Rub roast with salt, thyme, and black pepper.",
                        "step2": "Select sauté or brown on your pressure cooker, add oil to the pot, then add roast to the pot once oil is hot.",
                        "step3": "Brown for 10 minutes total – about 2-3 minutes per side.",
                        "step4": "Turn off your pressure cooker then add broth and wine to the pot.",
                        "step5": "Lock lid and set to high pressure for 50 minutes. When time is up, do a quick release then add potatoes, onions, carrots, and celery.",
                        "step6": "Lock lid and cook at high pressure for 10 minutes. When time is up, do a quick release and remove vegetables and meat to a serving platter.",
                        "step7": "Stir kale into broth in pressure cooker and let wilt for 1-2 minutes. Add kale to serving bowl, spoon some of the juices over top of meat and vegetables, and serve!",

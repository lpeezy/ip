        "baking": [
            {
                "title": "Banana Bread",
                "credit": "Elise Bauer",
                "creditURL": "https://www.simplyrecipes.com/recipes/banana_bread/",
                "serveSize": "1 loaf",
                "cookTime": "1 hour (10 min prep)",
                "img": "img/banana-bread.jpg",
                "ingredients": [
                    {
                        "ing1": "2 to 3 very ripe bananas, peeled",
                        "ing2": "1/3 cup melted butter",
                        "ing3": "1 teaspoon baking soda",
                        "ing4": "Pinch of salt",
                        "ing5": "3/4 cup sugar (1/2 cup if you would like it less sweet, 1 cup if more sweet",
                        "ing6": "1 large egg, beaten",
                        "ing7": "1 teaspoon vanilla extract",
                        "ing8": "1 1/2 cups of all-purpose flour",
                        "ing9": "",
                        "ing10": "",
                        "ing11": "",
                        "ing12": "",
                        "ing13": "",
                        "ing14": "",
                        "ing15": ""
                    }
                ],
                "instructions": [
                    {
                        "step1": "Preheat the oven to 350°F (175°C), and butter a 4x8-inch loaf pan.",
                        "step2": "In a mixing bowl, mash the ripe bananas with a fork until completely smooth. Stir the melted butter into the mashed bananas.",
                        "step3": "Mix in the baking soda and salt. Stir in the sugar, beaten egg, and vanilla extract. Mix in the flour.",
                        "step4": "Pour the batter into your prepared loaf pan. Bake for 50 minutes to 1 hour at 350°F (175°C), or until a tester inserted into the center comes out clean.",
                        "step5": "Remove from oven and let cool in the pan for a few minutes. Then remove the banana bread from the pan and let cool completely before serving. Slice and serve. (A bread knife helps to make slices that aren't crumbly.)",
                        "step6": "",
                        "step7": "",
                        "step8": "",
